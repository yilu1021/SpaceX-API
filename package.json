  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "blake3": "^2.1.4",
    "casbin": "^5.2.0",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.8.2",
    "fuzzball": "^1.3.0",
    "got": "^11.8.0",
    "ioredis": "^4.19.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
